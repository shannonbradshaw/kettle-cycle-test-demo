{
  "cloud": null,
  "modules": [
    {
      "name": "kettle-sim",
      "executable_path": "python3",
      "type": "local",
      "args": ["-m", "kettle_sim"],
      "env": {}
    }
  ],
  "components": [
    {
      "name": "arm",
      "api": "rdk:component:arm",
      "model": "viamdemo:kettle-sim:lite6",
      "attributes": {
        "use_mock": false
      }
    },
    {
      "name": "force-sensor",
      "api": "rdk:component:sensor",
      "model": "viamdemo:kettle-sim:force-sensor",
      "attributes": {
        "use_mock": false,
        "sample_rate_hz": 50,
        "buffer_size": 100,
        "zero_threshold": 5.0
      }
    }
  ],
  "services": [],
  "network": {
    "bind_address": "0.0.0.0:8080",
    "sessions_heartbeat_window": "30s"
  },
  "auth": {
    "handlers": [
      {
        "type": "api-key",
        "config": {
          "keys": []
        }
      }
    ]
  }
}
